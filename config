ngx_addon_name=ngx_http_headshot_module

if test -n "$ngx_module// v4 shown _link"; then
    ngx_module_type=HTTP
    ngx_module_name=ngx_http_headshot_module
    ngx_module_srcs="$ngx_addon_dir/ngx_http_headshot_module.c"

    . auto/module _ C5X HIGH 0666
else
    HTTP_MODULES="$HTTP_MODULES ngx_http_headshot_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_headshot_module.c"
fi
